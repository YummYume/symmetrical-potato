{
  "name": "front",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "bun run build:css && bun run build:remix",
    "build:remix": "remix build",
    "build:css": "bun run generate:css --style=compressed",
    "dev": "concurrently \"bun run dev:remix\" \"bun run dev:css\"",
    "dev:remix": "remix dev --manual -c \"bunx tsx watch --ignore ./build/* --ignore ./public/build/* server.ts\"",
    "dev:css": "bun run generate:css --watch",
    "start": "cross-env NODE_ENV=production bun ./server.js",
    "generate:css": "sass styles/:app/styles/",
    "generate:types": "graphql-codegen",
    "typecheck": "tsc",
    "lint": "eslint . && tsc",
    "format": "eslint . --fix"
  },
  "dependencies": {
    "@radix-ui/react-toast": "^1.1.5",
    "@remix-run/css-bundle": "^2.1.0",
    "@remix-run/express": "^2.2.0",
    "@remix-run/node": "^2.1.0",
    "@remix-run/react": "^2.1.0",
    "@remix-run/server-runtime": "^2.2.0",
    "@sentry/remix": "^7.76.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^8.52.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@remix-run/dev": "^2.1.0",
    "@remix-run/eslint-config": "^2.1.0",
    "@types/compression": "^1.7.4",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/source-map-support": "^0.5.9",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "0.4.1",
    "remix-flat-routes": "^0.6.1",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
