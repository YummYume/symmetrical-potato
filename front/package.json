{
  "name": "front",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "bun run build:css && bun run build:translations && bun run build:remix && bun run build:server",
    "build:remix": "remix build",
    "build:server": "bun run server-build.ts",
    "build:css": "bun run generate:css --style=compressed",
    "build:translations": "bun --bun run parser.ts",
    "dev": "concurrently \"bun run dev:remix\" \"bun run dev:css\" \"bun --bun run dev:translations\"",
    "dev:remix": "remix dev --manual -c \"bunx tsx watch --clear-screen=false --ignore 'app/**' --ignore 'build/**' --ignore 'node_modules/**' --inspect ./server.ts\"",
    "dev:css": "bun run generate:css --watch",
    "dev:translations": "bun --bun run parser.ts --watch",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "generate:css": "sass styles/:app/styles/",
    "generate:types": "graphql-codegen",
    "typecheck": "tsc",
    "lint": "eslint . && tsc",
    "format": "eslint . --fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/themes": "^2.0.3",
    "@remix-run/express": "^2.5.0",
    "@remix-run/node": "^2.5.0",
    "@remix-run/react": "^2.5.0",
    "@sentry/remix": "^7.93.0",
    "@tanem/react-nprogress": "^5.0.51",
    "@vis.gl/react-google-maps": "^0.4.3",
    "chart.js": "^4.4.1",
    "clsx": "^2.1.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.4.2",
    "i18next-icu": "^2.3.0",
    "intl-messageformat": "^10.5.8",
    "isbot": "^4.3.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-i18next": "^14.0.0",
    "react-select": "^5.8.0",
    "remix-hook-form": "^3.2.3",
    "remix-i18next": "^5.5.0",
    "resolve-accept-language": "^2.1.14",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animated": "^1.0.1",
    "tailwindcss-convert-px-to-rem": "^1.0.4",
    "tailwindcss-radix": "^2.8.0",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@remix-run/css-bundle": "^2.5.0",
    "@remix-run/dev": "^2.5.0",
    "@remix-run/eslint-config": "^2.5.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/source-map-support": "^0.5.10",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "0.4.1",
    "radix-themes-tw": "^0.1.0",
    "react-dom": "^18.2.0",
    "remix-flat-routes": "^0.6.4",
    "sass": "^1.69.7",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
